<h1>Listing all Users</h1>
<p> For now, nothing fancy, and display a few fields </p>
<!-- lbelater - confirm the delete -->
<!-- also notice method is :delete, but in action controler its method is destroy -->
  <% @users.each do |user| %>
  <%= user.first_name %>
  <%= user.last_name %>
  <%= link_to "Edit", edit_user_path(user.id) %>
  <%= link_to "Delete", user_path(user.id), method: :delete %>
  </br>
<% end %>
</br>
<%= link_to "New User", new_user_path %>




